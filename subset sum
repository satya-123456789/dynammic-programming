def subset_sum(arr,k):
    n=len(arr)
    if k > sum(arr):
        return 0
    elif n==0:
        return 0
    dp=[[0]*(k+1) for i in range(n)]
    dp[0][0]=1
    if arr[0] <= k:
        dp[0][arr[0]]=1
    for i in range(1,n):
        for j in range(k+1):
            dp[i][j]=dp[i-1][j]+dp[i-1][j-arr[i]] if j-arr[i] >=0 else 0
    return dp[n-k][k]
res = subset_sum([1,2,3,1,4],6)
print(res)


#Expected output :
#2