def rod(prices, n):
    dp = [-float('inf')] * (n + 1)
    dp[0] = 0 
    for i in range(1, n + 1):
        for length in range(1, i + 1):
            dp[i] = max(dp[i], prices[length - 1] + dp[i - length])
    
    return dp[n]
prices = [1, 5, 8, 9, 10, 17, 17, 20]
length_of_rod = 8
max_price = rod(prices, length_of_rod)
print("Maximum price:", max_price)



#Expected output:
#Maximum price: 22